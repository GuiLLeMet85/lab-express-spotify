<div id ="tracks">
    <header>
        <h2>Title</h2>
        <h2>Listen</h2>

    </header>
 <button class="backBtn" onclick="window.location = document.referrer">GO BACK</button>

    {{#each tracks}}
        <div class="track">
          <div class="track-name">
            <h3>-  {{this.name}}</h3>
          </div>
          <div class="preview-song">
            {{#if this.preview_url}}
            <audio controls src="{{this.preview_url}}">Your browser does not support the
            <code>audio</code> element.</audio>
            {{else}}
            <h5>Error: Preview not aviable</h5>
            {{/if}}
          </div>
        </div>
      {{/each}}
     
</div>  



